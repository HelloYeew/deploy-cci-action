{
  "name": "deploy-cci-action",
  "version": "1.0.0",
  "private": true,
  "description": "Action for deploying to Cloud Container Instance(CCI)",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "package": "ncc build src/main.ts -o dist --license licenses.txt",
    "test": "jest",
    "lint": "eslint src test --ext ts",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "all": "npm run lint && npm run format && npm run build && npm run package"
  },
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/tool-cache": "^1.7.2",
    "@huaweicloud/huaweicloud-sdk-core": "^3.0.37-rc",
    "@huaweicloud/huaweicloud-sdk-eip": "^3.0.37-rc",
    "@huaweicloud/huaweicloud-sdk-iam": "^3.0.37-rc",
    "@huaweicloud/huaweicloud-sdk-vpc": "^3.0.37-rc",
    "@vercel/ncc": "^0.33.3",
    "prettier": "^2.6.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "yaml": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/mime": "^2.0.3",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "jest": "^27.5.1",
    "mime": "^3.0.0"
  }
}
