{
  "name": "deploy-cci-action",
  "version": "1.0.0",
  "private": true,
  "description": "Action for deploying to Cloud Container Instance(CCI)",
  "main": "lib/main.js",
  "scripts": {
    "dev": " ts-node-dev src/main.ts",
    "build": "tsc",
    "package": "ncc build src/main.ts -o dist --license licenses.txt",
    "test": "jest --collectCoverage",
    "lint": "eslint src test --ext ts",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "all": "npm run lint && npm run format && npm run build && npm run package"
  },
  "dependencies": {
    "@actions/core": "^1.9.0",
    "@actions/tool-cache": "^2.0.1",
    "@huaweicloud/huaweicloud-sdk-core": "^3.0.47-rc",
    "@huaweicloud/huaweicloud-sdk-eip": "^3.0.47-rc",
    "@huaweicloud/huaweicloud-sdk-iam": "^3.0.47-rc",
    "@huaweicloud/huaweicloud-sdk-vpc": "^3.0.47-rc",
    "@types/mime": "^2.0.3",
    "mime": "^3.0.0",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.19.0",
    "jest": "^28.1.2",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
